
# CANable Candlelight Makefile

######################################
#
# This makefile compiles for the CANable board from 
# Jhoinrch which has a 25 MHz quartz
#
# Compile this by typing:
# make -s -f Make_G431_Candle_Jhoinrch
#
#######################################

# TARGET_BOARD and TARGET_MCU: maximum 24 characters!

TARGET_FIRMWARE = Candlelight
TARGET_BOARD    = Jhoinrch
TARGET_MCU      = STM32G431xx

# This board has a 25 MHz quartz.
QUARTZ_FREQU    = 25000000

# name of the compiled files (*.bin, *.hex, *.elf)
TARGET_FILE = STM32G431_Candlelight2.5_Jhoinrch

include GCC_Rules.mk